<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <family name="alatsi">
        <!-- regular -->
        <font
            android:font="@font/alatsi_regular"
            android:fontStyle="normal"
            android:fontWeight="400"
            app:font="@font/alatsi_regular"
            app:fontStyle="normal"
            app:fontWeight="400" />
    </family>

    <family name="open_sans_condensed">
        <!-- light -->
        <font
            android:font="@font/opensanscondensed_light"
            android:fontStyle="normal"
            android:fontWeight="300"
            app:font="@font/opensanscondensed_light"
            app:fontStyle="normal"
            app:fontWeight="300" />

        <font
            android:font="@font/opensanscondensed_lightitalic"
            android:fontStyle="normal"
            android:fontWeight="300"
            app:font="@font/opensanscondensed_lightitalic"
            app:fontStyle="normal"
            app:fontWeight="300" />


        <font
            android:font="@font/opensanscondensed_bold"
            android:fontStyle="normal"
            android:fontWeight="700"
            app:font="@font/opensanscondensed_bold"
            app:fontStyle="normal"
            app:fontWeight="700" />

    </family>

</font-family>
